text
@/* $Id$ */
/**************************************************************************
* Programa stub: Resuelve polinomios de grados 0 analíticamente *
* empleando la librería grado0. Además usa la librería salir para terminar*
* el programa. *
**************************************************************************/
#include "grado4.h"

int main(int argc, char **argv)
{
    num_tipo_raiz tipo;
    int i, grado = argc - 2;
    real c[MAX_COEF];
    real x[MAX_RAICES];

    despedidas();

    switch (argc) {
    case 1:
        error1();

    case 2:
        for (i = 0; i < argc - 1; i++)
            c[i] = atof(argv[i + 1]);

        for (; i < MAX_COEF; ++i)
            c[i] = 0.;

        tipo = constante(c, x);
        imprime_raices(c, x, grado, tipo);
        visto_bueno(c, x, grado, tipo);
        break;

    default:
        errorn();
        break;
    }

    salir();
    return (0);
}
@


